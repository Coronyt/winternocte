func update_cells(canvas):
	var x_ind = 0
	var y_ind = 0
	for row in canvas.get_children():
		#print(row)
		for cell in row.get_children():
			#print(cell)
			#print("initializing cell at x" + str(x_ind) + ", y" + str(y_ind))
